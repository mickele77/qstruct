#!/bin/bash

export DB_NAME=timber.db
export SQLCMD="/usr/bin/sqlite3 $DB_NAME"

rm $DB_NAME

$SQLCMD "create table woodEssence(idWoodEssence INTEGER PRIMARY KEY, nameWoodEssence TEXT);"
$SQLCMD "insert into woodEssence (idWoodEssence, nameWoodEssence) values(1, 'Conifere');"
$SQLCMD "insert into woodEssence (idWoodEssence, nameWoodEssence) values(2, 'Latofoglie');"

$SQLCMD "create table type1(idType1 INTEGER PRIMARY KEY, nameType1 TEXT, uni TEXT, uni_resistance TEXT);"
$SQLCMD "insert into type1 (idType1, nameType1, uni, uni_resistance) values(1, 'Legno massiccio', 'UNI EN 385', 'UNI EN 338:2004');"
$SQLCMD "insert into type1 (idType1, nameType1, uni, uni_resistance) values(2, 'Legno lamellare', 'UNI EN 14080', 'UNI EN 1194:2000');"

$SQLCMD "create table type2(idType2 INTEGER PRIMARY KEY, nameType2 TEXT, type1 INTEGER);"
