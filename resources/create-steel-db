$SQLCMD "create table bolt (id INTEGER PRIMARY KEY, class TEXT, nut TEXT, fyb DOUBLE, ftb DOUBLE);"
$SQLCMD "insert into bolt (id, class, nut, fyb, ftb) values (1, '4.6', '4', 2.40e+8, 4.00e+8);"
