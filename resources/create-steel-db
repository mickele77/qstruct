#!/bin/bash

export DB_NAME=steel.db
export SQLCMD="/usr/bin/sqlite3 $DB_NAME"

rm $DB_NAME

$SQLCMD "create table type (idType INTEGER PRIMARY KEY, nameType TEXT);"
$SQLCMD "insert into type (idType, nameType) values (1, 'Profilo laminato a caldo con sezione aperta');"
$SQLCMD "insert into type (idType, nameType) values (2, 'Profilo laminato a caldo con sezione chiusa');"

$SQLCMD "create table uni (idUNI INTEGER PRIMARY KEY, nameUNI TEXT, type INTEGER);"
$SQLCMD "insert into uni (idUNI, nameUNI, type) values (1, 'UNI EN 10025-2', 1);"
$SQLCMD "insert into uni (idUNI, nameUNI, type) values (2, 'UNI EN 10025-3', 1);"
$SQLCMD "insert into uni (idUNI, nameUNI, type) values (3, 'UNI EN 10025-4', 1);"
$SQLCMD "insert into uni (idUNI, nameUNI, type) values (4, 'UNI EN 10025-5', 1);"
$SQLCMD "insert into uni (idUNI, nameUNI, type) values (5, 'UNI EN 10210-1', 2);"
$SQLCMD "insert into uni (idUNI, nameUNI, type) values (6, 'UNI EN 10219-1', 2);"

$SQLCMD "create table steel (id INTEGER PRIMARY KEY, name TEXT, uni INTEGER, description TEXT, fyk DOUBLE, ftk DOUBLE, fyk40 DOUBLE, ftk40 DOUBLE);"
